<svg version="1.1"
  baseProfile="full"
  width="420" height="400"
  xmlns="http://www.w3.org/2000/svg">
  <pong-ball id='primary-ball'
    @register={{action registerTypeApi 'ball'}} />
  <pong-paddle id='opponent-paddle' @position={{opponentPosition}}
    @register={{action registerTypeApi 'leftPaddle'}} />
  <pong-paddle id='player-paddle' @position={{playerPosition}}
    @register={{action registerTypeApi 'rightPaddle'}} />
</svg>
